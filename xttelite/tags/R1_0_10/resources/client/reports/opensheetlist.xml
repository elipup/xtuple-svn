  <name>detail</name>
  <sql>select tehead_id,sheet_number,weekending,emp_code,sum(dollars) as dollars,status from 
(
select tehead_id,tehead_number as sheet_number,tehead_weekending as weekending,
         teitem_qty as hours,emp_code,teitem_total as dollars,
         tehead_status as status
         --,tehead_payable_status as pay_status 
         from te.tehead,te.teitem,emp
         where tehead_id = teitem_tehead_id 
         and teitem_emp_id = emp_id
         order by tehead_number

         ) rollup
         group by tehead_id,sheet_number,weekending,emp_code,status;

</sql>
