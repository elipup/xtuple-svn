 <querysource>
  <name>detail</name>
  <sql>SELECT vend_name,
       apchk_number,
       formatDate(apchk_checkdate) AS f_checkdate,
       formatMoney(apchk_amount) AS f_amount,
       apchk_amount
  FROM vend, apchk
 WHERE ((apchk_vend_id=vend_id)
   AND (apchk_journalnumber=&lt;? value(&quot;journalNumber&quot;) ?>) )
ORDER BY apchk_number;
    </sql>
 </querysource>
 <querysource>
  <name>head</name>
  <sql>SELECT (jrnluse_use || TEXT(jrnluse_number)) AS f_number,
       formatDate(jrnluse_date) AS f_date
  FROM jrnluse
 WHERE ((jrnluse_use='AP-CK')
   AND (jrnluse_number=&lt;? value(&quot;journalNumber&quot;) ?>));
</sql>
 </querysource>
