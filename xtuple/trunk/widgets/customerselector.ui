 <class>CustomerSelector</class>
 <widget class="QWidget" name="CustomerSelector">
  <property name="geometry">
   <rect>
    <x>0</x>
    <y>0</y>
    <width>500</width>
    <height>52</height>
   </rect>
  </property>
  <layout class="QHBoxLayout" name="horizontalLayout">
   <property name="margin">
    <number>0</number>
   </property>
   <item>
    <widget class="XComboBox" name="_select">
     <item>
      <property name="text">
       <string>All Customers</string>
      </property>
     </item>
     <item>
      <property name="text">
       <string>Select</string>
      </property>
     </item>
     <item>
      <property name="text">
       <string>Customer Group</string>
      </property>
     </item>
     <item>
      <property name="text">
       <string>Customer Type</string>
      </property>
     </item>
     <item>
      <property name="text">
       <string>Customer Type Pattern</string>
      </property>
     </item>
    </widget>
   </item>
   <item>
    <widget class="QStackedWidget" name="_selectStack">
     <property name="sizePolicy">
      <sizepolicy hsizetype="Expanding" vsizetype="Fixed">
       <horstretch>0</horstretch>
       <verstretch>0</verstretch>
      </sizepolicy>
     </property>
     <property name="currentIndex">
      <number>0</number>
     </property>
     <widget class="QWidget" name="_all"/>
     <widget class="QWidget" name="_selectedCustomer">
      <layout class="QGridLayout" name="gridLayout">
       <property name="margin">
        <number>0</number>
       </property>
       <item row="0" column="0">
        <widget class="CustInfo" name="_cust"/>
       </item>
      </layout>
     </widget>
     <widget class="QWidget" name="_selectedCustomerGroup">
      <layout class="QGridLayout" name="gridLayoutGroup">
       <item row="0" column="0">
        <widget class="XComboBox" name="_customerGroup">
         <property name="enabled">
          <bool>true</bool>
         </property>
         <property name="type">
          <enum>XComboBox::CustomerGroups</enum>
         </property>
        </widget>
       </item>
      </layout>
     </widget>
     <widget class="QWidget" name="_selectedCustomerType">
      <layout class="QGridLayout" name="gridLayout1">
       <item row="0" column="0">
        <widget class="XComboBox" name="_customerTypes">
         <property name="enabled">
          <bool>true</bool>
         </property>
         <property name="type">
          <enum>XComboBox::CustomerTypes</enum>
         </property>
        </widget>
       </item>
      </layout>
     </widget>
     <widget class="QWidget" name="_customerTypePattern">
      <layout class="QGridLayout" name="gridLayout_2">
       <item row="0" column="0">
        <widget class="XLineEdit" name="_customerType">
         <property name="enabled">
          <bool>true</bool>
         </property>
        </widget>
       </item>
      </layout>
     </widget>
    </widget>
   </item>
   <item>
    <spacer name="horizontalSpacer">
     <property name="orientation">
      <enum>Qt::Horizontal</enum>
     </property>
     <property name="sizeHint" stdset="0">
      <size>
       <width>0</width>
       <height>10</height>
      </size>
     </property>
    </spacer>
   </item>
  </layout>
 </widget>
 <customwidgets>
  <customwidget>
   <class>CustInfo</class>
   <extends>QWidget</extends>
   <header>custcluster.h</header>
  </customwidget>
  <customwidget>
   <class>XComboBox</class>
   <extends>QComboBox</extends>
   <header>xcombobox.h</header>
  </customwidget>
  <customwidget>
   <class>XLineEdit</class>
   <extends>QLineEdit</extends>
   <header>xlineedit.h</header>
   <container>1</container>
  </customwidget>
 </customwidgets>
 <resources/>
 <connections>
  <connection>
   <sender>_select</sender>
   <signal>currentIndexChanged(int)</signal>
   <receiver>_selectStack</receiver>
   <slot>setCurrentIndex(int)</slot>
   <hints>
    <hint type="sourcelabel">
     <x>112</x>
     <y>20</y>
    </hint>
    <hint type="destinationlabel">
     <x>265</x>
     <y>23</y>
    </hint>
   </hints>
  </connection>
 </connections>
