<!DOCTYPE openRPTDef>
<report>
 <title>Sample Check Format - 3 Part - MC - Remit</title>
 <name>APCheck</name>
 <description>Sample Check Format - 3 Part - MC - Uses primary Vendor or address with names: REMIT, Remit, or remit if one exists.</description>
 <size>Letter</size>
 <portrait/>
 <topmargin>0</topmargin>
 <bottommargin>0</bottommargin>
 <rightmargin>0</rightmargin>
 <leftmargin>50</leftmargin>
 <querysource>
  <name>Head</name>
  <sql>SELECT -- just vendors

  CASE WHEN UPPER(vendaddr_code) = 'REMIT' THEN
    formatAddr(
    addr.addr_line1,
    addr.addr_line2,
    addr.addr_line3,
    (addr.addr_city || ', ' || 
    addr.addr_state || ', ' || 
    addr.addr_postalcode),
    addr.addr_country)
  ELSE 
    formatAddr(vend_address1, vend_address2, vend_address3, 
    ( vend_city || ', ' || vend_state || ' ' || vend_zip ), vend_country)
  END 
  AS check_address,

  CASE WHEN UPPER(vendaddr_code) = 'REMIT' THEN
    vendaddrinfo.vendaddr_name
  ELSE 
    vend.vend_name
  END AS checkrecip_name, 
  vend_address1, 
  vend_address2, vend_address3,
  vend_city || ', ' || vend_state || ' ' || vend_zip AS address4,
  formatDate(checkhead_checkdate) AS f_checkdate,
  formatMoney(checkhead_amount) AS f_amount,
  initcap(spellAmount(checkhead_amount, curr_id)) AS f_words,
  CASE WHEN(checkhead_void) THEN text('V O I D')
  else text('')
  END AS f_void,
  checkhead_number,
  checkhead_for AS memo,
  vend_number AS checkrecip_number,
  checkhead_for,
  curr_symbol,
  curr_abbr,
  curr_name
FROM checkhead, vend LEFT JOIN
     vendaddrinfo ON ((vend.vend_id=vendaddrinfo.vendaddr_vend_id)
		  AND (UPPER(vendaddr_code)='REMIT')) LEFT JOIN
     addr ON (addr_id=vendaddr_addr_id),
     bankaccnt, curr_symbol
WHERE ( (checkhead_recip_id=vend_id)
  AND (checkhead_recip_type='V')
  AND (checkhead_curr_id = curr_id)
  AND (checkhead_id=&lt;? value("checkhead_id") ?>) )

UNION

SELECT	-- everyone other than vendors
  formatAddr(
  addr.addr_line1,
  addr.addr_line2,
  addr.addr_line3,
  (addr.addr_city || ', ' || 
  addr.addr_state || ', ' || 
  addr.addr_postalcode),
  addr.addr_country) AS check_address,
  checkrecip_name, 
  addr.addr_line1,
  addr.addr_line2,
  addr.addr_line3,
  (addr.addr_city || ', ' ||
  addr.addr_state || ', ' || addr.addr_postalcode) AS address4,
  formatDate(checkhead_checkdate) AS f_checkdate,
  formatMoney(checkhead_amount) AS f_amount,
  initcap(spellAmount(checkhead_amount, curr_id)) AS f_words,
  CASE WHEN(checkhead_void) THEN text('V O I D') else text('') END AS f_void,
  checkhead_number,
  checkhead_for AS memo,
  checkrecip_number,
  checkhead_for,
  curr_symbol,
  curr_abbr,
  curr_name
FROM checkhead LEFT JOIN
     checkrecip ON ((checkhead_recip_id=checkrecip_id)
		AND (checkhead_recip_type=checkrecip_type)) LEFT JOIN
     addr ON (addr_id = checkrecip_addr_id),
     bankaccnt, curr_symbol
WHERE ((checkhead_recip_type != 'V')
  AND  (checkhead_curr_id = curr_id)
  AND  (checkhead_id=&lt;? value("checkhead_id") ?>) )

LIMIT 1;</sql>
 </querysource>
 <querysource>
  <name>TopDetail2</name>
  <sql>SELECT&#xd; -- only have top part for vendors?
--VOUCHER-------------&#xd;
 1 as sequence_value, &#xd;
  1 as ord, &#xd;
  checkitem_invcnumber, &#xd;
  checkitem_ponumber, &#xd;
  formatMoney(checkitem_amount) as f_amount, &#xd;
  'Invoice#: ' || vohead_invcnumber AS vohead_invcnumber, &#xd;
  formatDate(vohead_docdate), &#xd;
  vohead_reference, &#xd;
  formatMoney(checkitem_amount),&#xd;
  'Voucher: ' || checkitem_vouchernumber AS vouchernumber,&#xd;
  formatMoney(apopen_amount) as amount,&#xd;
  formatMoney(checkitem_discount) AS disc_cred&#xd;
FROM checkitem, vohead, apopen &#xd;
WHERE (&#xd;
(checkitem_checkhead_id= &lt;? value("checkhead_id") ?>) &#xd;
AND checkitem_vouchernumber = vohead_number&#xd;
AND apopen_docnumber = checkitem_vouchernumber&#xd;
AND apopen_doctype = 'V')&#xd;
&#xd;
UNION&#xd;
&#xd;
SELECT&#xd;
--DEBIT MEMO -------------------------&#xd;
 1 as sequence_value, &#xd;
  2 as ord, &#xd;
  checkitem_invcnumber, &#xd;
  checkitem_ponumber, &#xd;
  formatMoney(checkitem_amount) as f_amount, &#xd;
 'Debit Memo PO#: ' || checkitem_ponumber AS vohead_invcnumber, &#xd;
 '' as vohead_docdate, &#xd;
 'Debit Memo: ' || checkitem_vouchernumber as vohead_reference, &#xd;
  formatMoney(checkitem_amount),&#xd;
  checkitem_vouchernumber AS vouchernumber,&#xd;
  formatMoney(apopen_amount) as amount,&#xd;
  formatMoney(checkitem_discount) AS disc_cred&#xd;
FROM checkhead, checkitem, apopen &#xd;
WHERE (&#xd;
(checkitem_checkhead_id= &lt;? value("checkhead_id") ?>) &#xd;
and checkitem_checkhead_id = checkhead_id&#xd;
AND (checkhead_recip_type='V')
and checkitem_vouchernumber = apopen_docnumber&#xd;
AND apopen_doctype = 'D')&#xd;
&#xd;
UNION&#xd;
&#xd;
SELECT &#xd;
--CREDITs--------------------------&#xd;
 1 as sequence_value, &#xd;
 3 as ord, &#xd;
 checkitem_invcnumber, &#xd;
 checkitem_ponumber, &#xd;
 formatMoney(checkitem_amount) as f_amount, &#xd;
 'Invoice#: ' || vohead_invcnumber AS vohead_invcnumber, &#xd;
 formatDate(vohead_docdate), &#xd;
 'Credit Applied: ' || apapply_source_doctype || ' ' || apapply_source_docnumber  AS vohead_reference, &#xd;
 '' as checkitem_amount,&#xd;
 'Voucher ' || checkitem_vouchernumber AS vouchernumber,&#xd;
 '' AS amount,&#xd;
 formatMoney((apapply_amount)) as disc_cred&#xd;
FROM checkitem, vohead, apapply &#xd;
WHERE (&#xd;
(checkitem_checkhead_id=&lt;? value("checkhead_id") ?>) &#xd;
AND checkitem_vouchernumber = vohead_number &#xd;
AND apapply_target_docnumber = checkitem_vouchernumber )&#xd;
&#xd;
&#xd;
UNION &#xd;
--BLANK LINES FOR SPACING------------------&#xd;
select &#xd;
 sequence_value, &#xd;
 4 as ord, &#xd;
 '' as checkitem_invcnumber, &#xd;
 '' as checkitem_ponumber, &#xd;
 '' as f_amount, &#xd;
 '' as vohead_invcnumber, &#xd;
 '' as vohead_docdate, &#xd;
 '' as vohead_reference, &#xd;
 '' as checkitem_amount,&#xd;
 '' as vouchernumber,&#xd;
 '' as amount,&#xd;
 '' as disc_cred&#xd;
FROM sequence &#xd;
&#xd;
ORDER BY ord &#xd;
LIMIT 16;&#xd;
&#xd;
</sql>
 </querysource>
