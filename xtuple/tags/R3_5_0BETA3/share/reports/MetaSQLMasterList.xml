 <querysource>
  <name>Detail</name>
  <sql>SELECT metasql.*,
       CASE WHEN nspname = 'public' THEN ''
            ELSE nspname
       END AS schema
  FROM metasql
  JOIN pg_class ON (metasql.tableoid=pg_class.oid)
  JOIN pg_namespace ON (relnamespace=pg_namespace.oid)
ORDER BY schema, metasql_group, metasql_name;</sql>
