<widget class="QDialog">
    <property name="name">
        <cstring>ReportParameter</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>458</width>
            <height>212</height>
        </rect>
    </property>
    <property name="caption">
        <string>Parameter</string>
    </property>
    <hbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QLayoutWidget">
            <property name="name">
                <cstring>layout1</cstring>
            </property>
            <grid>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QLabel" row="0" column="0">
                    <property name="name">
                        <cstring>_lblName</cstring>
                    </property>
                    <property name="text">
                        <string>Name:</string>
                    </property>
                </widget>
                <widget class="QLabel" row="1" column="0">
                    <property name="name">
                        <cstring>_lblDescrip</cstring>
                    </property>
                    <property name="text">
                        <string>Description:</string>
                    </property>
                    <property name="alignment">
                        <set>AlignTop</set>
                    </property>
                </widget>
                <widget class="QTextEdit" row="1" column="1">
                    <property name="name">
                        <cstring>_tbDescrip</cstring>
                    </property>
                </widget>
                <widget class="QLineEdit" row="0" column="1">
                    <property name="name">
                        <cstring>_leName</cstring>
                    </property>
                </widget>
            </grid>
        </widget>
        <widget class="QLayoutWidget">
            <property name="name">
                <cstring>layout2</cstring>
            </property>
            <vbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>_btnOk</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;OK</string>
                    </property>
                </widget>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>_btnCancel</cstring>
                    </property>
                    <property name="text">
                        <string>&amp;Cancel</string>
                    </property>
                </widget>
                <spacer>
                    <property name="name">
                        <cstring>spacer1</cstring>
                    </property>
                    <property name="orientation">
                        <enum>Vertical</enum>
                    </property>
                    <property name="sizeType">
                        <enum>Expanding</enum>
                    </property>
                    <property name="sizeHint">
                        <size>
                            <width>20</width>
                            <height>40</height>
                        </size>
                    </property>
                </spacer>
            </vbox>
        </widget>
    </hbox>
</widget>
<connections>
    <connection>
        <sender>_btnOk</sender>
        <signal>clicked()</signal>
        <receiver>ReportParameter</receiver>
        <slot>accept()</slot>
    </connection>
    <connection>
        <sender>_btnCancel</sender>
        <signal>clicked()</signal>
        <receiver>ReportParameter</receiver>
        <slot>reject()</slot>
    </connection>
</connections>
<tabstops>
    <tabstop>_leName</tabstop>
    <tabstop>_tbDescrip</tabstop>
    <tabstop>_btnOk</tabstop>
    <tabstop>_btnCancel</tabstop>
</tabstops>
<includes>
    <include location="local" impldecl="in implementation">reportparameter.ui.h</include>
</includes>
<slots>
    <slot returnType="QString">paramName()</slot>
    <slot returnType="QString">paramDescription()</slot>
    <slot>setParamName( const QString &amp; text )</slot>
    <slot>setParamDescription( const QString &amp; text )</slot>
</slots>
<layoutdefaults spacing="6" margin="11"/>
</UI>
