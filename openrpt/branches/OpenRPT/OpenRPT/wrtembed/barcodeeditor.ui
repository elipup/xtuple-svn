<connections>
    <connection>
        <sender>cbFormat</sender>
        <signal>activated(int)</signal>
        <receiver>BarcodeEditor</receiver>
        <slot>cbFormat_activated(int)</slot>
    </connection>
    <connection>
        <sender>sliderMaxVal</sender>
        <signal>valueChanged(int)</signal>
        <receiver>labelMaxVal</receiver>
        <slot>setNum(int)</slot>
    </connection>
    <connection>
        <sender>sliderMaxVal</sender>
        <signal>sliderMoved(int)</signal>
        <receiver>BarcodeEditor</receiver>
        <slot>sliderMaxVal_sliderMoved(int)</slot>
    </connection>
    <connection>
        <sender>buttonOk</sender>
        <signal>clicked()</signal>
        <receiver>BarcodeEditor</receiver>
        <slot>accept()</slot>
    </connection>
    <connection>
        <sender>buttonCancel</sender>
        <signal>clicked()</signal>
        <receiver>BarcodeEditor</receiver>
        <slot>reject()</slot>
    </connection>
</connections>
