</variables>
<slots>
    <slot>_btnAdd_clicked()</slot>
    <slot>_lbColors_dblClick( QListBoxItem * item )</slot>
    <slot>_btnEdit_clicked()</slot>
    <slot>_btnDelete_clicked()</slot>
    <slot>init( QMap&lt;QString, QColor&gt; * cmap )</slot>
</slots>
