<connections>
    <connection>
        <sender>_btnClose</sender>
        <signal>clicked()</signal>
        <receiver>ColorList</receiver>
        <slot>accept()</slot>
    </connection>
    <connection>
        <sender>_btnAdd</sender>
        <signal>clicked()</signal>
        <receiver>ColorList</receiver>
        <slot>_btnAdd_clicked()</slot>
    </connection>
    <connection>
        <sender>_btnEdit</sender>
        <signal>clicked()</signal>
        <receiver>ColorList</receiver>
        <slot>_btnEdit_clicked()</slot>
    </connection>
    <connection>
        <sender>_btnDelete</sender>
        <signal>clicked()</signal>
        <receiver>ColorList</receiver>
        <slot>_btnDelete_clicked()</slot>
    </connection>
    <connection>
        <sender>_lbColors</sender>
        <signal>doubleClicked(QListBoxItem*)</signal>
        <receiver>ColorList</receiver>
        <slot>_lbColors_dblClick(QListBoxItem*)</slot>
    </connection>
</connections>
